
<%if current_user %>
  <%= link_to "View Events", events_path %> <br>
  <% unless !current_user.is_admin? %>
    <%= link_to "Add Events",new_event_path %> <br>
  <%end %>

  <br>

  <%= link_to "My Reviews", my_reviews_path(id: current_user.id) %> <br>
  <%= link_to "View All Reviews", reviews_path %> <br><br>

  <%= link_to "My Bookings", my_bookings_path(id: current_user.id) %> <br>
  <% unless !current_user.is_admin? %>
    <%= link_to "View All Bookings", all_bookings_path %> <br><br>
  <%end %>

  <% unless !current_user.is_admin? %>
    <%= link_to "Search Attendees by Event", users_path(display: 1) %> <br><br>
  <%end %>

  <% unless !current_user.is_admin? %>
    <%= link_to "View all Attendees", users_path %> <br><br>
  <%end %>

  <% unless !current_user.is_admin? %>
    <%= link_to "View Rooms", rooms_path %> <br>
    <%= link_to "Create Room", new_room_path %><br>
  <%end %>
  <br>
  <%= link_to 'View Profile', profile_path(id: current_user.id) %> <br>
  <%= link_to 'Edit Profile', edit_user_path(id: current_user.id) %><br>
<%else %>
  <%= link_to "Signup", signup_path%> or
  <%= link_to "Login",login_path %>
<%end %>
